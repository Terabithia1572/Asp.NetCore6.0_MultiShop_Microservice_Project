@model List<MultiShop.RapidApiWebUI.Models.ECommerceViewModel.Product>
@{
    Layout = null;
}

<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Ürün Başlığı</th>
            <th>Ürün Puanı</th>
            <th>Ürün Fiyatı</th>
            <th>Ürün Detayları</th>
          
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.product_id</td>
                <td>@item.product_title</td>
                <td>@item.product_rating</td>
                <td>@item.price</td>
                <td><a href="#" class="btn btn-primary">Ürün Detayları</a></td>
            </tr>
        }
    </table>
</div>
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />