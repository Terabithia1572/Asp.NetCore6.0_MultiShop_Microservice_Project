
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/UILayout/_UILayout.cshtml";
}

<div class="container-fluid">
    <div class="row px-xl-5">
        <div class="col-12">
            <nav class="breadcrumb bg-light mb-30">
                <a class="breadcrumb-item text-dark" href="#">Ana Sayfa</a>
                <span class="breadcrumb-item active">Ürün Listesi</span>
            </nav>
        </div>
    </div>
</div>

@await Component.InvokeAsync("_CarouselDefaultComponentPartial") <!-- Burada Biz Carousel kodlarını _CarouselDefaultComponentPartial Tuttuk -->
@await Component.InvokeAsync("_FeatureDefaultComponentPartial") <!-- Burada Biz Feature kodlarını _FeatureDefaultComponentPartial Tuttuk -->
@await Component.InvokeAsync("_CategoriesDefaultComponentPartial") <!-- Burada Biz Kategoriler kodlarını _CategoriesDefaultComponentPartial Tuttuk -->
@await Component.InvokeAsync("_FeatureProductsDefaultComponentPartial") <!-- Burada Biz Öne Çıkan Ürünler kodlarını _FeatureProductsDefaultComponentPartial Tuttuk -->")