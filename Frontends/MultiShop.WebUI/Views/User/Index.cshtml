@model UserDetailViewModel
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /><br /><br />
    <h3>Kullanıcı Profil Bilgisi</h3>
    <div  class="alert alert-danger">Kullanıcı Id: @Model.ID</div>
    <div  class="alert alert-info">Kullanıcı Adı: @Model.Username</div>
    <div  class="alert alert-warning">Ad: @Model.Name</div>
    <div  class="alert alert-success">Soyad: @Model.Surname</div>
    <div  class="alert alert-dark">E-Mail Adresi: @Model.Email</div>
</div>
