@model List<ResultCategoryDTO>
@{
    Layout = null;
}

<div class="container">
    <br />
    <br />
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Kategori Adı</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CategoryID</td>
                <td>@item.CategoryName</td>
            </tr>
            
        }
    </table>
</div>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />